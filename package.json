{
  "name": "image-croper",
  "version": "0.1.0",
  "description": "Non-destructive crop images in Obsidian",
  "scripts": {
    "dev": "esbuild src/main.ts --bundle --outfile=dist/main.js --watch --external:obsidian",
    "build": "esbuild src/main.ts --bundle --outfile=dist/main.js --minify --external:obsidian --format=cjs"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "obsidian": "latest"
  }
}
